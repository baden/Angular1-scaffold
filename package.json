{
  "name": "angular1-scaffold",
  "version": "1.0.0",
  "description": "Angular 1 scaffold with Typescript and ng-metadata",
  "main": "src/main.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngParty/Angular1-scaffold.git"
  },
  "scripts": {
    "start": "npm run server",
    "build": "NODE_ENV=production webpack --config webpack-build.config.js",
    "build:dev": "webpack --progress --profile --colors --display-error-details --display-cached",
    "test": "karma start",
    "test:ci": "karma start --browsers Firefox",
    "test:watch": "npm run test -- --auto-watch --no-single-run",
    "clean": "rimraf doc/ coverage/ dist/",
    "server": "webpack-dev-server --inline --progress --profile --colors --watch --display-error-details --display-cached --content-base src/"
  },
  "keywords": [],
  "contributors": [
    "Martin Hochel <hochelmartin@gmail.com>",
    "Mario Vejlupek <mario@vejlupek.cz>"
  ],
  "license": "ISC",
  "dependencies": {
    "@types/angular": "1.6.36",
    "@types/core-js": "0.9.43",
    "angular": "1.6.6",
    "core-js": "2.5.1",
    "ng-metadata-baden-fix1": "4.0.1-fix1",
    "reflect-metadata": "0.1.10",
    "rxjs": "5.5.2",
    "tslib": "1.8.0"
  },
  "devDependencies": {
    "@angular/compiler": "4.4.6",
    "@angular/core": "4.4.6",
    "@types/angular-mocks": "1.5.11",
    "@types/jasmine": "2.6.2",
    "@types/lodash": "4.14.80",
    "@types/node": "8.0.47",
    "angular-mocks": "1.6.6",
    "awesome-typescript-loader": "3.2.3",
    "codelyzer": "3.2.2",
    "copy-webpack-plugin": "4.2.0",
    "css-loader": "0.28.7",
    "html-webpack-plugin": "2.30.1",
    "istanbul-instrumenter-loader": "3.0.0",
    "jasmine-core": "2.8.0",
    "json-loader": "0.5.7",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "1.1.1",
    "karma-firefox-launcher": "1.0.1",
    "karma-jasmine": "1.1.0",
    "karma-mocha-reporter": "2.2.5",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.5",
    "lodash": "4.17.4",
    "node-sass": "4.5.3",
    "raw-loader": "0.5.1",
    "remap-istanbul": "0.9.5",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "source-map-loader": "0.2.3",
    "style-loader": "0.19.0",
    "tslint": "5.8.0",
    "tslint-loader": "3.5.3",
    "typescript": "2.5.3",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.3",
    "zone.js": "0.8.18"
  },
  "engines": {
    "node": ">= 6.x",
    "npm": ">= 3"
  }
}
